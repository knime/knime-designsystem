{
  "name": "@knime/kds-styles",
  "version": "0.0.8",
  "description": "Package containing the design tokens and icons for the KNIME Design System",
  "repository": {
    "type": "git",
    "url": "https://github.com/knime/knime-designsystem.git",
    "directory": "packages/styles"
  },
  "license": "GPL 3 and Additional Permissions according to Sec. 7 (SEE the file LICENSE)",
  "author": "KNIME AG, Zurich, Switzerland",
  "type": "module",
  "scripts": {
    "build": "pnpm run --parallel --aggregate-output /^build:.*/",
    "build:icons": "node src/processSVGs.js",
    "build:tokens": "node src/buildTokens.js",
    "build:css": "node src/copyCss.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "modern-normalize": "3.0.1"
  },
  "devDependencies": {
    "@knime/styles": "^1.10.0",
    "@tokens-studio/sd-transforms": "^2.0.1",
    "@types/jsdom": "catalog:",
    "@types/node": "catalog:",
    "consola": "catalog:",
    "jsdom": "catalog:",
    "style-dictionary": "^5.0.4",
    "svgo": "^4.0.0",
    "typescript": "catalog:"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./index.css": "./dist/css/index.css",
    "./kds-variables.css": "./dist/tokens/css/_variables.css",
    "./kds-properties.css": "./dist/tokens/css/_properties.css",
    "./kds-legacy-theme.css": "./dist/css/legacy/legacy-theme.css",
    "./img/*": "./dist/img/*.svg",
    "./img/*/def": "./dist/img/*/def.ts"
  },
  "engines": {
    "node": "24.10.0",
    "npm": "please-use-pnpm"
  },
  "packageManager": "pnpm@10.18.3"
}
