{
  "kds": {
    "color": {
      "spinner": {
        "progress": {
          "onFilled": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "0",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.color.text-and-icon.primary-inverted}"
          },
          "onTransparent": {
            "$type": "color",
            "$value": "{kds.color.text-and-icon.neutral}"
          }
        },
        "track": {
          "onFilled": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.24",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.color.text-and-icon.primary-inverted}"
          },
          "onTransparent": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.24",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.color.text-and-icon.neutral}"
          }
        }
      },
      "desktop-header": {
        "background": {
          "default": {
            "$type": "color",
            "$value": "{kds.core.colors.neutral.0}"
          },
          "tab": {
            "$type": "color",
            "$value": "{kds.color.surface.default}"
          }
        },
        "text-and-icon": {
          "neutral": {
            "$type": "color",
            "$value": "{kds.core.colors.neutral.100}"
          },
          "tab": {
            "$type": "color",
            "$value": "{kds.color.text-and-icon.neutral}"
          }
        }
      }
    }
  }
}