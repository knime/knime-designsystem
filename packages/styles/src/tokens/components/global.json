{
  "kds": {
    "color": {
      "DesktopHeader": {
        "background": {
          "default": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.875}",
            "$description": "Default background color for the DesktopHeader component."
          },
          "muted": {
            "$type": "color",
            "$value": "{kds.color.surface.default}",
            "$description": "Muted background color for the DesktopHeader component."
          },
          "accent": {
            "$type": "color",
            "$value": "{kds.color.surface.default}",
            "$description": "Accent background color for the DesktopHeader component."
          }
        },
        "text-and-icon": {
          "neutral": {
            "$type": "color",
            "$value": "{kds.color.text-and-icon.neutral}",
            "$description": "Neutral text and icon color for the DesktopHeader component."
          },
          "muted": {
            "$type": "color",
            "$value": "{kds.color.text-and-icon.muted}",
            "$description": "Muted text and icon color for the DesktopHeader component."
          }
        },
        "border": {
          "default": {
            "$type": "color",
            "$value": "{kds.color.border.subtle}",
            "$description": "Default border color for the DesktopHeader component."
          }
        }
      },
      "TableHeader": {
        "background": {
          "default": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.25}",
            "$description": "Default background color for the TableHeader component."
          }
        }
      },
      "ToggleSwitch": {
        "knob": {
          "initial": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.25}",
            "$description": "Initial knob color for the ToggleSwitch component."
          },
          "disabled": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.175}",
            "$description": "Disabled knob color for the ToggleSwitch component."
          }
        },
        "icon": {
          "on": {
            "$type": "color",
            "$value": "{kds.color.text-and-icon.primary}",
            "$description": "Icon color when the ToggleSwitch is on."
          },
          "off": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.375}",
            "$description": "Icon color when the ToggleSwitch is off."
          },
          "disabled": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.275}",
            "$description": "Icon color when the ToggleSwitch is disabled."
          }
        }
      },
      "LoadingSpinner": {
        "progress": {
          "onPrimary": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "0",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.color.text-and-icon.primary-inverted}",
            "$description": "Progress indicator color for LoadingSpinner on primary backgrounds."
          },
          "onSurface": {
            "$type": "color",
            "$value": "{kds.color.text-and-icon.neutral}",
            "$description": "Progress indicator color for LoadingSpinner on surface backgrounds."
          }
        },
        "track": {
          "onPrimary": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.24",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.color.text-and-icon.primary-inverted}",
            "$description": "Track color for LoadingSpinner on primary backgrounds."
          },
          "onSurface": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.24",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.color.text-and-icon.neutral}",
            "$description": "Track color for LoadingSpinner on surface backgrounds."
          }
        }
      },
      "SkeletonBar": {
        "background": {
          "default": {
            "$type": "color",
            "$value": "{kds.color.skeleton.default}",
            "$description": "Skeleton animation gradient for the SkeletonBar component."
          }
        }
      },
      "Tooltip": {
        "background": {
          "default": {
            "$type": "color",
            "$value": "{kds.color.background-static.overlay}",
            "$description": "Default background color for the Tooltip component."
          }
        },
        "text-and-icon": {
          "neutral": {
            "$type": "color",
            "$value": "{kds.core.color.neutral.50}",
            "$description": "Neutral text and icon color for the Tooltip component."
          }
        }
      },
      "TextHighlight": {
        "background": {
          "default": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "0.6",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{kds.core.color.blue.325}",
            "$description": "Text background color for mark text or highlighting searchterms"
          }
        }
      }
    },
    "spacing": {
      "inputLabel": {
        "spacing-bottom": {
          "$type": "spacing",
          "$value": "{kds.core.size.scale} * 0.375",
          "$description": "Bottom spacing between an input label and its input field."
        }
      },
      "SubText": {
        "spacing-top": {
          "$type": "spacing",
          "$value": "{kds.core.size.scale} * 0.25",
          "$description": "Top spacing between SubText and its associated element."
        }
      }
    },
    "border": {
      "ResizeHandle": {
        "initial": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.neutral-bold-initial}",
            "width": "{kds.core.border-width.l}",
            "style": "solid"
          },
          "$description": "Default border style for the ResizeHandle component."
        },
        "hover": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.neutral-bold-hover}",
            "width": "{kds.core.border-width.l}",
            "style": "solid"
          },
          "$description": "Border style for the ResizeHandle component on hover."
        },
        "active": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.neutral-bold-active}",
            "width": "{kds.core.border-width.l}",
            "style": "solid"
          },
          "$description": "Border style for the ResizeHandle component when active."
        }
      },
      "ColorPickerHandle": {
        "initial": {
          "$type": "border",
          "$value": {
            "color": "{kds.core.color.neutral.25}",
            "width": "{kds.core.border-width.l}",
            "style": "solid"
          },
          "$description": "Default border style for the ColorPickerHandle in colorspace."
        }
      }
    },
    "box-shadow": {
      "knob": {
        "$type": "boxShadow",
        "$value": {
          "x": "0",
          "y": "1",
          "blur": "2",
          "spread": "0",
          "color": "{kds.color.elevation.knob}",
          "type": "dropShadow"
        },
        "$description": "Drop shadow for knob elements like the ToggleSwitch handle."
      }
    }
  }
}