{
  "kds": {
    "border-radius": {
      "container": {
        "none": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.0}",
          "$description": "0 px radius - perfectly sharp corners. Use when elements demands no rounding. e.g ListContainer component and TabBar-Item \n"
        },
        "0_12x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.12}",
          "$description": "2px radius - softens crisp elements while keeping a sharp feel\n"
        },
        "0_25x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.25}",
          "$description": "4 px - radius on small UI pieces and compact elements e.g Xsmall buttons, checkboxes\n"
        },
        "0_31x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.31}",
          "$description": "5px - A touch rounder than previous radius. use for elements that need a gentler feel e.g Focus outlines on Xsmall buttons\n\n"
        },
        "0_37x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.37}",
          "$description": "6px - balanced rounding for small to medium components e.g small and medium buttons and input fields\n"
        },
        "0_44x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.44}",
          "$description": "7px - A touch rounder than previous radius. use for elements that need a gentler feel e.g Focus outlines on small and medium buttons\n\n"
        },
        "0_50x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.50}",
          "$description": "8px - soft rounding for larger components e.g large buttons\n"
        },
        "0_56x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.56}",
          "$description": "9px - A touch rounder than previous radius. use for elements that need a gentler feel e.g Focus outlines on large buttons\n\n"
        },
        "1x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.100}",
          "$description": "16px - used on rounder corner for larger elements "
        },
        "pill": {
          "$type": "borderRadius",
          "$value": "1000",
          "$description": "1000 px - Makes circles or capsules e.g radiobuttons and Badge\n"
        }
      }
    },
    "border-width": {
      "base": {
        "none": {
          "$type": "borderWidth",
          "$value": "{kds.core.dimension.0}"
        },
        "s": {
          "$type": "borderWidth",
          "$value": "{kds.core.dimension.6}"
        },
        "m": {
          "$type": "borderWidth",
          "$value": "{kds.core.dimension.12}"
        }
      },
      "icon-stroke": {
        "s": {
          "$type": "borderWidth",
          "$value": "1"
        },
        "m": {
          "$type": "borderWidth",
          "$value": "1.25"
        },
        "l": {
          "$type": "borderWidth",
          "$value": "1.5"
        }
      }
    },
    "border": {
      "action": {
        "transparent": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.transparent}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "default": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.neutral}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "disabled": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.disabled}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "focused": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.focus.outline}",
            "width": "{kds.border-width.base.m}",
            "style": "solid"
          }
        },
        "selected": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.selected}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "error": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.danger-bold}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "primary": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.primary-bold}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "input": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.neutral}",
            "width": "{kds.border-width.base.s}"
          }
        }
      },
      "base": {
        "default": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.neutral}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "muted": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.muted}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "subtle": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.subtle}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "brand": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.brand}",
            "width": "{kds.core.dimension.6}",
            "style": "soild"
          }
        },
        "danger": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.danger}",
            "width": "{kds.core.dimension.6}",
            "style": "soild"
          },
          "$description": "Messages"
        },
        "warning": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.warning}",
            "width": "{kds.core.dimension.6}",
            "style": "soild"
          },
          "$description": "Messages"
        },
        "success": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.success}",
            "width": "{kds.core.dimension.6}",
            "style": "soild"
          },
          "$description": "Messages"
        },
        "info": {
          "$type": "border",
          "$value": {
            "color": "{kds.color.border.info}",
            "width": "{kds.core.dimension.6}",
            "style": "soild"
          },
          "$description": "Messages"
        }
      }
    }
  }
}