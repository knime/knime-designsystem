{
  "border-radius": {
    "container": {
      "none": {
        "$type": "borderRadius",
        "$value": "{dimension.0}"
      },
      "xs": {
        "$type": "borderRadius",
        "$value": "{dimension.12}"
      },
      "s": {
        "$type": "borderRadius",
        "$value": "{dimension.25}"
      },
      "m": {
        "$type": "borderRadius",
        "$value": "{dimension.50}"
      },
      "l": {
        "$type": "borderRadius",
        "$value": "{dimension.100}"
      },
      "pill": {
        "$type": "borderRadius",
        "$value": "1000"
      }
    }
  },
  "border-width": {
    "base": {
      "none": {
        "$type": "borderWidth",
        "$value": "{dimension.0}"
      },
      "s": {
        "$type": "borderWidth",
        "$value": "{dimension.6}"
      },
      "m": {
        "$type": "borderWidth",
        "$value": "{dimension.12}"
      }
    },
    "icon-stroke": {
      "s": {
        "$type": "borderWidth",
        "$value": "1"
      },
      "m": {
        "$type": "borderWidth",
        "$value": "1.25"
      },
      "l": {
        "$type": "borderWidth",
        "$value": "1.5"
      }
    }
  },
  "border": {
    "interactive": {
      "transparent": {
        "$type": "border",
        "$value": {
          "color": "{color.border.transparent}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "default": {
        "$type": "border",
        "$value": {
          "color": "{color.border.default}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "disabled": {
        "$type": "border",
        "$value": {
          "color": "{color.border.subtle}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "focused": {
        "$type": "border",
        "$value": {
          "color": "{color.border.focused}",
          "width": "{border-width.base.m}",
          "style": "solid"
        }
      },
      "active": {
        "$type": "border",
        "$value": {
          "color": "{color.border.active}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "error": {
        "$type": "border",
        "$value": {
          "color": "{color.border.error}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      }
    },
    "base": {
      "default": {
        "$type": "border",
        "$value": {
          "color": "{colors.neutral.20}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "subtle": {
        "$type": "border",
        "$value": {
          "color": "{color.border.subtle}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "muted": {
        "$type": "border",
        "$value": {
          "color": "{color.border.muted}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      }
    }
  }
}