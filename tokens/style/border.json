{
  "kds": {
    "border-radius": {
      "container": {
        "none": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.0}"
        },
        "0_12x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.12}"
        },
        "0_25x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.25}"
        },
        "0_37x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.37}"
        },
        "0_50x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.50}"
        },
        "1x": {
          "$type": "borderRadius",
          "$value": "{kds.core.dimension.100}"
        },
        "pill": {
          "$type": "borderRadius",
          "$value": "1000"
        }
      }
    },
    "border-width": {
      "base": {
        "none": {
          "$type": "borderWidth",
          "$value": "{kds.core.dimension.0}"
        },
        "s": {
          "$type": "borderWidth",
          "$value": "{kds.core.dimension.6}"
        },
        "m": {
          "$type": "borderWidth",
          "$value": "{kds.core.dimension.12}"
        }
      },
      "icon-stroke": {
        "s": {
          "$type": "borderWidth",
          "$value": "1"
        },
        "m": {
          "$type": "borderWidth",
          "$value": "1.25"
        },
        "l": {
          "$type": "borderWidth",
          "$value": "1.5"
        }
      }
    },
    "border": {
      "interactive": {
        "transparent": {
          "$type": "border",
          "$value": {
            "color": "{color.border.transparent}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "default": {
          "$type": "border",
          "$value": {
            "color": "{color.border.default}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "disabled": {
          "$type": "border",
          "$value": {
            "color": "{color.border.subtle}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "focused": {
          "$type": "border",
          "$value": {
            "color": "{color.border.focused}",
            "width": "{kds.border-width.base.m}",
            "style": "solid"
          }
        },
        "selected": {
          "$type": "border",
          "$value": {
            "color": "{color.border.active}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "error": {
          "$type": "border",
          "$value": {
            "color": "{color.border.error}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        }
      },
      "base": {
        "default": {
          "$type": "border",
          "$value": {
            "color": "{kds.core.colors.neutral.20}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "subtle": {
          "$type": "border",
          "$value": {
            "color": "{color.border.subtle}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        },
        "muted": {
          "$type": "border",
          "$value": {
            "color": "{color.border.muted}",
            "width": "{kds.border-width.base.s}",
            "style": "solid"
          }
        }
      }
    }
  }
}