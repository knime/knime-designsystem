{
  "color": {
    "base": {
      "bg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.0}"
        },
        "muted": {
          "$type": "color",
          "$value": "{core.color.neutral.10}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{core.color.neutral.20}"
        }
      },
      "fg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.90}",
          "$description": "main text color"
        },
        "muted": {
          "$type": "color",
          "$value": "{core.color.neutral.60}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{core.color.neutral.50}"
        }
      }
    },
    "interactive": {
      "fg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.10}"
        },
        "selected": {
          "$type": "color",
          "$value": "{core.color.blue.20}"
        }
      },
      "bg": {
        "primary": {
          "default": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "darken",
                  "value": ".012",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{core.color.yellow.50}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.modifiers.color.hover.high}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.primary.default}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{core.color.neutral.30}"
          }
        },
        "secondary": {
          "default": {
            "$type": "color",
            "$value": "{core.color.transparent}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "mix",
                  "value": "{interactive.modifiers.color.hover.low}",
                  "space": "lch",
                  "color": "{core.color.neutral.10}"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.secondary.default}"
          }
        },
        "tertiary": {
          "default": {
            "$type": "color",
            "$value": "{core.color.transparent}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "mix",
                  "value": "{interactive.modifiers.color.hover.low}",
                  "space": "lch",
                  "color": "{core.color.neutral.10}"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.tertiary.default}"
          }
        },
        "selectables": {
          "default": {
            "$type": "color",
            "$value": "{core.color.blue.60}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.modifiers.color.hover.high}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.selectables.default}"
          }
        }
      }
    },
    "function": {
      "success": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.green.40}"
          },
          "default-inverted": {
            "$type": "color",
            "$value": "{core.color.green.90}"
          }
        },
        "fg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.green.90}"
          },
          "default-inverted": {
            "$type": "color",
            "$value": "{core.color.green.20}"
          }
        }
      },
      "error": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.red.20}"
          },
          "default-inverted": {
            "$type": "color",
            "$value": "{core.color.red.60}"
          }
        },
        "fg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.red.70}"
          },
          "default-inverted": {
            "$type": "color",
            "$value": "{core.color.red.10}"
          }
        }
      },
      "info": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.blue.20}"
          },
          "default-inverted": {
            "$type": "color",
            "$value": "{core.color.blue.60}"
          }
        },
        "fg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.blue.80}"
          },
          "default-inverted": {
            "$type": "color",
            "$value": "{core.color.blue.10}"
          }
        }
      }
    }
  },
  "interactive": {
    "modifiers": {
      "lightness": {
        "low": {
          "$type": "other",
          "$value": "0.4"
        },
        "high": {
          "$type": "other",
          "$value": "0.24"
        }
      },
      "color": {
        "hover": {
          "low": {
            "$type": "other",
            "$value": "{interactive.modifiers.lightness.low}"
          },
          "high": {
            "$type": "other",
            "$value": "{interactive.modifiers.lightness.high}"
          }
        },
        "pressed": {
          "$type": "other",
          "$value": "{interactive.modifiers.lightness.low}"
        },
        "focused": {
          "$type": "other",
          "$value": "0"
        }
      },
      "disabled-alpha": {
        "$type": "other",
        "$value": "0.5"
      },
      "enabled-alpha": {
        "$type": "other",
        "$value": "1"
      }
    }
  },
  "core": {
    "box-shadow": {
      "intensity": {
        "$type": "other",
        "$value": "0.1"
      }
    }
  },
  "border": {
    "interactive": {
      "default": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.50}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "muted": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.30}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "focused": {
        "$type": "border",
        "$value": {
          "color": "{core.color.blue.60}",
          "width": "{border-width.base.m}",
          "style": "solid"
        }
      },
      "selected": {
        "$type": "border",
        "$value": {
          "color": "{core.color.blue.40}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      }
    },
    "base": {
      "default": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.20}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "subtle": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.40}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      }
    }
  },
  "elevation": {
    "low": {
      "$type": "boxShadow",
      "$value": [
        {
          "x": "0",
          "y": "1",
          "blur": "4",
          "spread": "0",
          "color": "rgba(0,0,0,0.48)",
          "type": "dropShadow"
        },
        {
          "x": "0",
          "y": "4",
          "blur": "12",
          "spread": "0",
          "color": "rgba(0,0,0,0.16)",
          "type": "dropShadow"
        }
      ]
    }
  }
}