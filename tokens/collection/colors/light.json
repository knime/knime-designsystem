{
  "color": {
    "base": {
      "bg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.100}"
        },
        "muted": {
          "$type": "color",
          "$value": "{core.color.neutral.90}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{core.color.neutral.80}"
        }
      },
      "fg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.10}",
          "$description": "main text color"
        },
        "muted": {
          "$type": "color",
          "$value": "{core.color.neutral.40}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{core.color.neutral.60}"
        }
      }
    },
    "interactive": {
      "bg": {
        "primary": {
          "default": {
            "$type": "color",
            "$value": "{core.color.yellow.50}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.modifiers.color.hover.high}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.primary.default}"
          },
          "disabled": {
            "$type": "color",
            "$value": "{core.color.neutral.70}"
          }
        },
        "secondary": {
          "default": {
            "$type": "color",
            "$value": "{core.color.transparent}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "mix",
                  "value": "{interactive.modifiers.color.hover.low}",
                  "space": "lch",
                  "color": "{core.color.neutral.10}"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.secondary.default}"
          }
        },
        "tertiary": {
          "default": {
            "$type": "color",
            "$value": "{core.color.transparent}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "mix",
                  "value": "{interactive.modifiers.color.hover.low}",
                  "space": "lch",
                  "color": "{core.color.neutral.10}"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.tertiary.default}"
          }
        },
        "selectables": {
          "default": {
            "$type": "color",
            "$value": "{core.color.blue.80}"
          },
          "hover": {
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "lighten",
                  "value": "{interactive.modifiers.color.hover.high}",
                  "space": "lch"
                }
              }
            },
            "$type": "color",
            "$value": "{color.interactive.bg.selectables.default}"
          }
        }
      },
      "fg": {
        "default": {
          "$type": "color",
          "$value": "{core.color.neutral.10}"
        },
        "selected": {
          "$type": "color",
          "$value": "{core.color.blue.30}"
        }
      }
    },
    "system": {
      "success": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.green.40}"
          }
        },
        "fg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.green.90}"
          }
        }
      },
      "error": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.red.30}"
          }
        },
        "fg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.red.80}"
          }
        }
      },
      "info": {
        "bg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.blue.80}"
          }
        },
        "fg": {
          "default": {
            "$type": "color",
            "$value": "{core.color.blue.40}"
          }
        }
      }
    }
  },
  "interactive": {
    "modifiers": {
      "lightness": {
        "low": {
          "$type": "other",
          "$value": "0.12"
        },
        "high": {
          "$type": "other",
          "$value": "0.24"
        }
      },
      "color": {
        "hover": {
          "low": {
            "$type": "other",
            "$value": "{interactive.modifiers.lightness.low}"
          },
          "high": {
            "$type": "other",
            "$value": "{interactive.modifiers.lightness.high}"
          }
        },
        "pressed": {
          "$type": "other",
          "$value": "{interactive.modifiers.lightness.low}"
        },
        "focused": {
          "$type": "other",
          "$value": "0"
        }
      }
    }
  },
  "core": {
    "box-shadow": {
      "intensity": {
        "$type": "other",
        "$value": "0.1"
      }
    }
  },
  "border": {
    "interactive": {
      "default": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.20}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "muted": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.60}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      },
      "focused": {
        "$type": "border",
        "$value": {
          "color": "{core.color.blue.60}",
          "width": "{border-width.base.m}",
          "style": "solid"
        }
      },
      "selected": {
        "$type": "border",
        "$value": {
          "color": "{core.color.blue.50}",
          "width": "{border-width.base.s}",
          "style": "solid"
        }
      }
    },
    "base": {
      "default": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.50}"
        }
      },
      "subtle": {
        "$type": "border",
        "$value": {
          "color": "{core.color.neutral.70}",
          "width": "1",
          "style": "solid"
        }
      }
    }
  },
  "elevation": {
    "low": {
      "$type": "boxShadow",
      "$value": [
        {
          "x": "0",
          "y": "1",
          "blur": "2",
          "spread": "0",
          "color": "rgba(0,0,0,0.24)",
          "type": "dropShadow"
        },
        {
          "x": "0",
          "y": "2",
          "blur": "8",
          "spread": "0",
          "color": "rgba(0,0,0,0.12)",
          "type": "dropShadow"
        }
      ]
    }
  }
}