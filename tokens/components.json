    "spinner": {
      "progress": {
        "onFilled": {
          "$type": "color",
          "$value": "{color.system.status-inverted.onNeutral}"
        },
        "onTransparent": {
          "$type": "color",
          "$value": "{color.system.status.onNeutral}"
        }
      },
      "track": {
