                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{color.system.status.neutral}"
        },
        "onTransparent": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "value": "0.24",
                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{color.system.status-inverted.neutral}"
        }
      }
    }
  }
}