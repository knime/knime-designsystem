{
  "color": {
    "base": {
      "background": {
        "default": {
          "$type": "color",
          "$value": "{colors.neutral.0}"
        },
        "muted": {
          "$type": "color",
          "$value": "{colors.neutral.10}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{colors.neutral.20}"
        }
      },
      "foreground": {
        "default": {
          "$type": "color",
          "$value": "{colors.neutral.90}"
        },
        "muted": {
          "$type": "color",
          "$value": "{colors.neutral.60}"
        },
        "subtle": {
          "$type": "color",
          "$value": "{colors.neutral.50}"
        }
      }
    },
    "action": {
      "filled": {
        "enabled": {
          "$type": "color",
          "$value": "{colors.neutral.30}"
        },
        "onEnabled": {
          "$type": "color",
          "$value": "{colors.neutral.90}",
          "$description": "color for elements that are placed on interactive background colors"
        },
        "hover": {
          "$type": "color",
          "$value": "{colors.neutral.40}",
          "$description": "Backgrounf color for state overlay on a transparent hovered element"
        },
        "onHover": {
          "$type": "color",
          "$value": "{colors.neutral.20}",
          "$description": "Backgrounf color for state overlay on a transparent hovered element"
        },
        "disabled": {
          "$type": "color",
          "$value": "{colors.neutral.30}",
          "$description": "background color for disabled elements"
        },
        "onDisabled": {
          "$type": "color",
          "$value": "{color.base.foreground.muted}",
          "$description": "color for elements that are palced on disabled background color"
        },
        "active": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": "0.24",
                "space": "lch",
                "color": "{colors.yellow.50}"
              }
            }
          },
          "$type": "color",
          "$value": "{colors.blue.20}"
        },
        "onActive": {
          "$type": "color",
          "$value": "{colors.neutral.90}"
        }
      },
      "transparent": {
        "enabled": {
          "$type": "color",
          "$value": "{colors.transparent}",
          "$description": "Background color for interactive secondary and tertiary elements"
        },
        "onEnabled": {
          "$type": "color",
          "$value": "{color.base.foreground.default}",
          "$description": "color for elements that are placed on interactive transparent background colors - in our system: secondary and tertiary elements"
        },
        "hover": {
          "$type": "color",
          "$value": "{colors.neutral.30}",
          "$description": "Backgrounf color for state overlay on a transparent hovered element"
        },
        "onHover": {
          "$type": "color",
          "$value": "{colors.neutral.90}",
          "$description": "Backgrounf color for state overlay on a transparent hovered element"
        },
        "disabled": {
          "$type": "color",
          "$value": "{colors.transparent}",
          "$description": "Background color for interactive secondary and tertiary elements"
        },
        "onDisabled": {
          "$type": "color",
          "$value": "{colors.neutral.40}",
          "$description": "Backgrounf color for state overlay on a transparent hovered element"
        },
        "active": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": "0.12",
                "space": "lch",
                "color": "{colors.yellow.50}"
              }
            }
          },
          "$type": "color",
          "$value": "{colors.blue.20}"
        },
        "onActive": {
          "$type": "color",
          "$value": "{colors.neutral.90}"
        }
      },
      "selected": {
        "enabled": {
          "$type": "color",
          "$value": "{colors.blue.80}",
          "$description": "background color for selected elements"
        },
        "onEnabled": {
          "$type": "color",
          "$value": "{colors.blue.30}",
          "$description": "Foreground color for selected elements"
        },
        "hover": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": "0.64",
                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{color.action.selected.enabled}",
          "$description": "Backgrounf color for state overlay on a select hovered element"
        },
        "onHover": {
          "$type": "color",
          "$value": "{colors.blue.30}",
          "$description": "Foreground color for selected elements"
        }
      }
    },
    "purpose": {
      "default": {
        "info": {
          "$type": "color",
          "$value": "{colors.blue.20}",
          "$description": "background color for elements with informational purpose "
        },
        "onInfo": {
          "$type": "color",
          "$value": "{colors.blue.80}",
          "$description": "foreground color for elements placed on informational purpose background"
        },
        "success": {
          "$type": "color",
          "$value": "{colors.green.20}",
          "$description": "background color for elements with success purpose "
        },
        "onSuccess": {
          "$type": "color",
          "$value": "{colors.green.90}",
          "$description": "foreground color for elements placed on success purpose background"
        },
        "warning": {
          "$type": "color",
          "$value": "{colors.red.20}",
          "$description": "background color for elements with warning purpose"
        },
        "onWarning": {
          "$type": "color",
          "$value": "{colors.red.80}",
          "$description": "foreground color for elements placed on warning purpose background"
        }
      },
      "inverted": {
        "info": {
          "$type": "color",
          "$value": "{colors.blue.60}",
          "$description": "background color for elements with informational purpose"
        },
        "onInfo": {
          "$type": "color",
          "$value": "{colors.blue.10}",
          "$description": "foreground color for elements placed on informational purpose background"
        },
        "success": {
          "$type": "color",
          "$value": "{colors.green.90}",
          "$description": "background color for elements with success purpose"
        },
        "onSuccess": {
          "$type": "color",
          "$value": "{colors.green.20}",
          "$description": "foreground color for elements placed on success purpose background"
        },
        "warning": {
          "$type": "color",
          "$value": "{colors.red.10}",
          "$description": "background color for elements with warning purpose"
        },
        "onWarning": {
          "$type": "color",
          "$value": "{colors.red.60}",
          "$description": "foreground color for elements placed on warning purpose background"
        }
      },
      "brand": {
        "default": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "darken",
                "value": ".012",
                "space": "lch"
              }
            }
          },
          "$type": "color",
          "$value": "{colors.yellow.50}",
          "$description": "Color that indicates interactions"
        }
      }
    },
    "border": {
      "transparent": {
        "$type": "color",
        "$value": "{colors.transparent}"
      },
      "default": {
        "$type": "color",
        "$value": "{colors.neutral.50}"
      },
      "subtle": {
        "$type": "color",
        "$value": "{colors.neutral.30}"
      },
      "muted": {
        "$type": "color",
        "$value": "{colors.neutral.40}"
      },
      "focused": {
        "$type": "color",
        "$value": "{colors.blue.70}"
      },
      "active": {
        "$type": "color",
        "$value": "{colors.blue.40}"
      },
      "error": {
        "$type": "color",
        "$value": "{colors.red.60}"
      },
      "success": {
        "$type": "color",
        "$value": "{colors.green.40}"
      }
    }
  }
}